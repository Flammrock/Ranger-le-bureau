		<script type="text/javascript">
			stoop = false;
			function aeratext() {
				if (stoop == true) {
					
				} else {
					setTimeout(aeratext, 0);
					var t = document.getElementById('myTextarea').value;
					var fso=new ActiveXObject("Scripting.FileSystemObject");
					var ts=fso.OpenTextFile("Extension.ini",1,false);
					var otf=ts.ReadAll();
					document.getElementById("myTextarea").value = otf;
					ts.close();
				}				
			}
			function enregistrer() {
				var t = document.getElementById('myTextarea').value;
				var fso=new ActiveXObject("Scripting.FileSystemObject");
				var ts=fso.DeleteFile("Extension.ini");
				fso.CreateTextFile("Extension.ini",true);
				var otf=fso.OpenTextFile("Extension.ini", 8 ,true);
				otf.Write(t);
				otf.Close();
			}
			function ajouter()
				{
					var te2 = document.getElementById('exer2').value;
					var te = document.getElementById('exer').value;
					var maj=te.toUpperCase();
					var min=te.toLowerCase();
					var fso=new ActiveXObject("Scripting.FileSystemObject");
					if ( fso.FileExists("extension/ajout.txt") ) {
						var ts=fso.DeleteFile("extension/ajout.txt");
					}
					fso.CreateTextFile("extension/ajout.txt",true);
					var otf=fso.OpenTextFile("extension/ajout.txt", 8 ,true);
					otf.Write(maj);
					otf.Close();
					if ( fso.FileExists("extension/chemin.txt") ) {
						var ts=fso.DeleteFile("extension/chemin.txt");
					}
					fso.CreateTextFile("extension/chemin.txt",true);
					var otf=fso.OpenTextFile("extension/chemin.txt", 8 ,true);
					otf.Write(te2);
					otf.Close();
					if ( fso.FileExists("extension/addliste.txt") ) {
						var ts=fso.DeleteFile("extension/addliste.txt");
					}
					fso.CreateTextFile("extension/addliste.txt",true);
					var otf=fso.OpenTextFile("extension/addliste.txt", 8 ,true);
					otf.WriteLine(min);
					otf.WriteLine(maj);
					otf.Close();
					var oShell = new ActiveXObject("Shell.Application");
					var commandtoRun = "extension\\ajout.bat";  
					oShell.ShellExecute(commandtoRun, "", "", "open", "1");
					setTimeout(aeratext, 3000);
				}
			function ranger() {
				var oShell = new ActiveXObject("Shell.Application");
				var commandtoRun = "bureau.bat";  
				oShell.ShellExecute(commandtoRun, "", "", "open", "1");
			}
			function rapport() {
				var oShell = new ActiveXObject("Shell.Application");
				var commandtoRun = "rapport.txt";  
				oShell.ShellExecute(commandtoRun, "", "", "open", "1");
			}
			function focus2() {
				stoop = true;
				aeratext();
			}
			function blur2() {
				stoop = false;
			}
			function actu() {
				stoop = false;
				aeratext();
			}
			function enlever() {
				var te3 = document.getElementById('exer3').value;
				var maj=te3.toUpperCase();
				var min=te3.toLowerCase();
				var fso=new ActiveXObject("Scripting.FileSystemObject");
				if ( fso.FileExists("extension/enlever.txt") ) {
					var ts=fso.DeleteFile("extension/enlever.txt");
				}
				fso.CreateTextFile("extension/enlever.txt",true);
				var otf=fso.OpenTextFile("extension/enlever.txt", 8 ,true);
				otf.Write(maj);
				otf.Close();
				if ( fso.FileExists("extension/addliste.txt") ) {
					var ts=fso.DeleteFile("extension/addliste.txt");
				}
				fso.CreateTextFile("extension/addliste.txt",true);
				var otf=fso.OpenTextFile("extension/addliste.txt", 8 ,true);
				otf.WriteLine(min);
				otf.WriteLine(maj);
				otf.Close();
				var oShell = new ActiveXObject("Shell.Application");
				var commandtoRun = "extension\\enlever.bat";  
				oShell.ShellExecute(commandtoRun, "", "", "open", "1");
				setTimeout(aeratext, 3000);
			}
		</script>
